{
  "version": 2,
  "builds": [
    {
      "src": "manage.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/server.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "myapp/wsgi.py"
    },
    {
      "src": "/api/(.*)",
      "dest": "api/server.py"
    }
  ],
  "env": {
    "PORT": "8000"
  }
}
